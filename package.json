{
  "name": "zkcloudworker",
  "version": "0.18.20",
  "type": "module",
  "description": "zkCloudWorker library",
  "author": "zkCloudWorker",
  "license": "Apache-2.0",
  "homepage": "https://docs.zkcloudworker.com",
  "scripts": {
    "compile": "rm -rf dist/lib && tsc && tsc --build tsconfig.web.json && node build-cjs.js",
    "login": "yarn npm login",
    "publish": "yarn compile && yarn npm publish --access public"
  },
  "keywords": [
    "javascript",
    "typescript",
    "mina-protocol",
    "zkcloudworker",
    "cloudworker",
    "cloud",
    "proving"
  ],
  "bugs": {
    "url": "https://github.com/zkcloudworker/zkcloudworker-lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkcloudworker/zkcloudworker-lib.git"
  },
  "main": "dist/web/index.js",
  "exports": {
    "types": "./dist/node/index.d.ts",
    "browser": "./dist/web/index.js",
    "node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "default": "./dist/web/index.js"
  },
  "types": "./dist/node/index.d.ts",
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^4.1.2"
  },
  "peerDependencies": {
    "o1js": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "esbuild": "^0.24.0",
    "o1js": "^2.1.0",
    "typescript": "^5.7.2"
  },
